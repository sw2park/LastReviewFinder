<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="WishList">
	<insert id="insertWishList" parameterType="hashmap">
		INSERT INTO MOVIE_FAVORITE_LIST values(#{userid},#{movie_num})
	</insert>
	<select id="checkWishList" parameterType="hashmap" resultType="_int">
		SELECT count(*) FROM MOVIE_FAVORITE_LIST WHERE userid=#{userid} AND MOVIE_NUM = #{movie_num}	
	</select>
</mapper>